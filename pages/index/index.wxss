/**index.wxss**/
.container{
  width: 100%;
  margin: 0;
  padding: 0;
  position: relative;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
.background {
  height: 1334rpx;
  width: 750rpx;
  position: fixed;
  background-size:100% 100%;
  z-index: -1;
}

.time {
  font-size: 140rpx;
  color: white;
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top:140rpx;
}

/* cell */
.item {
  width: 100%;
  height:150rpx;
  position: relative;
 
}
/* cell 产品图 */
.iconImageView {
  width: 200rpx;
  height: 200rpx;
  top: 50rpx;
  left: 40rpx;
  /* background-color:orange; */
  position: absolute;
}
.timeview {
  top:50rpx;
  left:60rpx;
  height: 50rpx;
  position: absolute;
}
.productNameText {
  font-size: 70rpx;
  display: flex;
  color: white;
}
.switch {
  top: 70rpx;
  right: 30rpx;
  height: 100rpx;
  width: 150rpx;
  position: absolute;
}

.buttonimage{
  width: 100%;
  height: 50%;
}

page {
  background-color: #4A4A4A;
}